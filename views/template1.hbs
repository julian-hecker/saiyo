<style>
    * {
        box-sizing: border-box;
    }

    html {
        height: 100%;
        max-width: 100%;
    }

    body {
        margin: 0;
        padding: 0;
        height: 100%;
        transform-style: preserve-3d;
        background-color: rgb(0, 0, 0);
    }

    .page {
        margin: 0;
        padding: 0;

        height: 100%;
        overflow-y: scroll;
        overflow-x: hidden;
        font-family: arial, helvetica-serif;
        color: black;
        background: white;
    }

    h1 {
        font-size: 2.5em;
        line-height: 1.2em;
    }

    p {
        font-size: 1.2em;
    }

    .center {
        display: flex;
        justify-content: center;
        align-items: center;
    }

    a:hover {
        color: rgb(0, 116, 217);
    }


    .content {
        padding: 1.5em 2em;
        margin: 0 auto;
        background: rgba(255, 255, 255, 0.75);
        box-shadow: .5em .5em .5em rgba(0, 0, 0, .4);
        border-color: black;
        border-style: solid;
        border-width: 2px;
    }

    .section {
        position: relative;
        padding: 2rem;
    }
</style>

<div class="page">
    <div id="pageone" class="section">
        <div class="content">
            <h1>{{data.name}}</h1>
            {{#each data}}
            <h2>{{@key}}</h2>
            <p>{{this}}</p>
            {{/each}}
            <h3>{{data.email}} {{data.phone}}</h3>
        </div>
    </div>

</div>